
<h1> Add a new Flat </h1>


<%= simple_form_for [:account, @flat], html: {class: 'form'} do |f| %>
  <%= f.input :title %>
  <%= f.input :description %>
  <%= f.input :beds %>
  <%= f.input :street %>
  <%= f.input :city %>
  <%= f.input :postcode %>
  <%= f.input :price %>
  <%= f.input :picture %>

    <%= f.submit "enregistrer", class: "form-actions" %>

<% end %>

